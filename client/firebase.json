{
    "hosting": [
        {
            "target": "development",
            "public": "web-build",
            "rewrites": [
                {
                    "source": ".well-known/assetlinks.json",
                    "destination": "/development/assetlinks.json"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "appAssociation": "NONE"
        },
        {
            "target": "preview",
            "public": "web-build",
            "rewrites": [
                {
                    "source": ".well-known/assetlinks.json",
                    "destination": "/preview/assetlinks.json"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "appAssociation": "NONE"
        },
        {
            "target": "production",
            "public": "web-build",
            "rewrites": [
                {
                    "source": ".well-known/assetlinks.json",
                    "destination": "/production/assetlinks.json"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "appAssociation": "NONE"
        }
    ]
}
