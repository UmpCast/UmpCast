{
    "expo": {
        "name": "UmpCast",
        "slug": "umpcast",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "DEVELOPMENT": {
                "server_uri": "http://localhost:8000/graphql/"
            },
            "PRODUCTION": {
                "server_uri": null
            }
        },
        "entryPoint": "./src/App.tsx"
    }
}
