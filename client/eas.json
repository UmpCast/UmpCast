{
    "cli": {
        "version": ">= 0.42.4",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_ENV": "development"
            }
        },
        "production": {
            "autoIncrement": true,
            "channel": "production",
            "env": {
                "APP_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "service-account-key.json"
            },
            "ios": {
                "ascAppId": "6444740365"
            }
        }
    }
}
