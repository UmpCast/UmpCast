{
    "cli": {
        "version": ">= 0.42.4"
    },
    "build": {
        "production": {
            "releaseChannel": "production",
            "env": {
                "APP_ENV": "production"
            }
        },
        "preview": {
            "releaseChannel": "preview",
            "distribution": "internal",
            "env": {
                "APP_ENV": "preview"
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_ENV": "development"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "playstore-service-account.json",
                "track": "internal"
            }
        }
    }
}
