{
    "cli": {
        "version": ">= 0.40.0"
    },
    "build": {
        "production": {
            "releaseChannel": "production",
            "env": {
                "APP_ENV": "production"
            }
        },
        "development": {
            "releaseChannel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_ENV": "development"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "eas-service-account.json",
                "track": "internal"
            }
        }
    }
}
