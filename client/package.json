{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "prepare": "cd ../ && husky install client/.husky",
        "start": "expo start --web",
        "test": "jest",
        "test:dev": "jest --watch --maxWorkers=1",
        "download-schema": "apollo client:download-schema --endpoint=http://localhost:8000/graphql/ --excludes='src/apollo/localSchema.tsx' src/schema.graphql",
        "codegen": "rimraf src/**/__generated__ && apollo client:codegen --target=typescript --localSchemaFile=src/schema.graphql",
        "sync-schema": "yarn download-schema && yarn codegen",
        "tsnode": "NODE_ENV=development TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true ts-node -r tsconfig-paths/register"
    },
    "dependencies": {
        "@apollo/client": "^3.4.16",
