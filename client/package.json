{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --web",
    "test": "jest --watch",
    "download-schema": "apollo client:download-schema --endpoint=http://localhost:8000/graphql/ --excludes='src/app/cache/index.ts' src/schema.graphql",
    "codegen": "apollo client:codegen --outputFlat --target=typescript --localSchemaFile=src/schema.graphql --no-addTypename src/generated",
    "codegen:clean": "rimraf src/**/types src/generated",
    "sync-schema": "(yarn download-schema & yarn codegen:clean) && yarn codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "@hookform/resolvers": "^2.7.1",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.0.4",
    "apollo": "^2.33.6",
    "expo": "~42.0.1",
    "graphql": "^15.5.1",
    "native-base": "^3.0.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.12.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "cypress": "^8.2.0",
    "jest-expo": "^42.1.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.0.0"
  },
  "private": true
}
