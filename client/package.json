{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "prepare": "cd ../ && husky install client/.husky",
        "start": "expo start --web",
        "test": "jest",
        "test:dev": "jest --watch",
        "download-schema": "apollo client:download-schema --endpoint=http://localhost:8000/graphql/ --excludes='src/app/cache/index.ts' src/schema.graphql",
        "codegen": "apollo client:codegen --no-addTypename --target=typescript --localSchemaFile=src/schema.graphql && rimraf __generated__",
        "sync-schema": "yarn download-schema && yarn codegen"
    },
    "dependencies": {
        "@apollo/client": "^3.4.7",
        "@hookform/resolvers": "^2.7.1",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/native-stack": "^6.0.4",
        "apollo": "^2.33.6",
        "expo": "~42.0.1",
        "expo-constants": "~11.0.1",
        "graphql": "^15.5.1",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.2",
        "native-base": "^3.0.7",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hook-form": "^7.12.2",
        "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
        "react-native-safe-area-context": "3.2.0",
        "react-native-screens": "~3.4.0",
        "react-native-svg": "12.1.1",
        "react-native-web": "~0.13.12",
        "styled-components": "^5.3.0",
        "styled-system": "^5.1.5"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@testing-library/jest-native": "^4.0.2",
        "@testing-library/react-native": "^7.2.0",
        "@types/jest": "^27.0.1",
        "@types/react": "~16.9.35",
        "@types/react-native": "~0.64.13",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "cypress": "^8.2.0",
        "eslint": "7.8.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^13.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.24.1",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "jest-expo": "^42.1.0",
        "prettier": "2.3.2",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "typescript": "~4.0.0"
    },
    "private": true
}
