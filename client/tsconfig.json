{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "resolveJsonModule": true,
        "strict": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./*"]
        },
        "jsx": "react-jsx",
        "typeRoots": ["./types", "./node_modules/@types"],
        "types": ["node", "jest", "@testing-library/jest-native"],
    },
    "exclude": [
        "types"
    ]
}
